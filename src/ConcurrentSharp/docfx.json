{
  "metadata": [
    {
			"src": [
				{
					"files": [ "**.cs" ],
					"src": "..",
									"exclude": [
										"**/obj/**",
										"**/bin/**",
										"../docs/_site/**",
										"**Bom**",
										"**Test**",
									]
				}
      ],
      "dest": "obj/api"
    }
  ],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml"
				],
				"cwd": "obj"
			},
			{
				"files": [
					"api/*.md",
					"articles/**.md",
					"toc.yml",
					"*.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"dest": "../../docs",
			"globalMetadata": {
				"_appTitle": "ConcurrentSharp",
				"_appFaviconPath": "images/ConcurrentSharp.ico",
				"_appLogoPath": "images/ConcurrentSharp.png",
				"_appFooter": "Copyright (c) 2017 Troy Willmot"
			},
		"template": [ "statictoc", "memberpage/2.24.0/content" ],
	}
}